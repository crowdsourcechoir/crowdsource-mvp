{
  "name": "crowdsource-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null; next dev -p 3000",
    "fresh": "rm -rf .next && npm run build && next dev -p 3000",
    "reboot": "rm -rf node_modules .next && npm install && next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@supabase/supabase-js": "^2.97.0",
    "jszip": "^3.10.1",
    "next": "14.2.35",
    "openai": "^6.22.0",
    "qr-code-styling": "^1.9.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-jsx": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
